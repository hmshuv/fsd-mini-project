{
    "name": "aimed-api",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "TS_NODE_TRANSPILE_ONLY=1 node --watch --loader ts-node/esm src/server.ts",
        "start": "node dist/server.js",
        "build": "tsc -p .",
        "migrate": "prisma migrate deploy || prisma migrate reset --force && prisma db seed",
        "seed": "prisma db seed",
        "prisma": {
            "seed": "node --loader ts-node/esm ../prisma/seed.ts"
        }
    },
    "dependencies": {
        "@fastify/cors": "^11.1.0",
        "@fastify/helmet": "^13.0.2",
        "@fastify/jwt": "^9.1.0",
        "@fastify/rate-limit": "^10.1.0",
        "@prisma/client": "^6.19.0",
        "@types/bcrypt": "^6.0.0",
        "bcrypt": "^6.0.0",
        "fastify": "^5.6.2",
        "multer": "^2.0.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "prisma": "^6.19.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    }
}
